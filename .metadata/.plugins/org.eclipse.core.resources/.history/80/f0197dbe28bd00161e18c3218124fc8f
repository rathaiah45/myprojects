package com.cts.dao;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestBody;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RequestMethod;
import org.springframework.web.bind.annotation.ResponseBody;

import com.cts.dao.responses.RegistreationSuccess;

@Controller
@RequestMapping(value="/",method=RequestMsethod.POST)
public class RegistreationResponse {

	public @ResponseBody RegistrationSuccess(@RequestBody Registreation register)
	{
		RegistreationSuccess success=new RegistreationSuccess();
		return success;
	}
}
